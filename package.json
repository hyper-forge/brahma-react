{
  "name": "brahma-react",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently --names \"vite,server\" -c \"cyan,green\" \"vite\" \"nodemon --watch src --ext js,ts --exec node src/server.js\"",
    "build": "tsc && vite build && node esbuild.config.cjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:server": "node src/server.js",
    "start": "node bundle.main.js"
  },
  "dependencies": {
    "brahma-firelight": "^1.5.20",
    "mime-types": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
